<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.AddCourseDialog">
    <children>
        <Button fx:id="add_course_btn" layoutX="327.0" layoutY="159.0" mnemonicParsing="false" onMouseClicked="#addCourse" text="Add Course" AnchorPane.bottomAnchor="20.0" AnchorPane.rightAnchor="20.0" />

        <Label layoutX="14.0" layoutY="14.0" text="Course Name:" AnchorPane.leftAnchor="50.0" AnchorPane.topAnchor="100.0" />

        <TextField fx:id="input_course_name" layoutX="50.0" layoutY="118.0" prefHeight="26.0" prefWidth="200.0" promptText="Enter Course Name" />

        <Label layoutX="50.0" layoutY="144.0" text="Select Time:" AnchorPane.leftAnchor="50.0" AnchorPane.topAnchor="175.0" />

        <ComboBox fx:id="combo_time" editable="true" layoutX="50.0" layoutY="192.0" prefHeight="28.0" prefWidth="200.0" promptText="Time Course Takes Place" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0" AnchorPane.topAnchor="192.0" />
        
    </children>
</AnchorPane>
